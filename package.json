{
  "name": "ace-gremlin-editor",
  "version": "1.0.0",
  "description": "基于 Ace 的 Gremlin 编辑器",
  "keywords": [
    "ace",
    "gremlin",
    "editor",
    "graph"
  ],
  "homepage": "https://github.com/baizn/ace-gremlin-editor",
  "bugs": {
    "url": "https://github.com/baizn/ace-gremlin-editor/issues"
  },
  "repository": {
    "type": "https",
    "url": "https://github.com/baizn/ace-gremlin-editor"
  },
  "license": "MIT",
  "scripts": {
    "start": "umi dev",
    "build": "npm run clean && father build",
    "clean": "rimraf es esm lib dist",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/g6ChartPlugin.min.js",
  "files": [
    "package.json",
    "es",
    "lib",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "father": "^2.29.6",
    "rimraf": "^3.0.2",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.14",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.14",
    "yorkie": "^2.0.0"
  }
}
